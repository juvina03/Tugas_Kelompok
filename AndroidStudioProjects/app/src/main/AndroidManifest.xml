<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bankqueue">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BankQueue">
        <activity android:name=".Customerpart"></activity>
        <activity android:name=".Employeepart" />
        <activity android:name=".Chooseservice" />
        <activity android:name=".login_as_customer" />
        <activity android:name=".login_as_employee" />
        <activity android:name=".login_as">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.BankQueue.NoActionBar" />
    </application>

</manifest>